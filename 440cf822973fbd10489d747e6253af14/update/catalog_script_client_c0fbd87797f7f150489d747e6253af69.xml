        <name>QuandoCompletoPreenchido</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }

	var Rua = g_form.getValue('logradouro');
	var Numero = g_form.getValue('complemento');
	var Bairro = g_form.getValue('bairro');
	var Cidade = g_form.getValue('cidade');
	var Estado = g_form.getValue('estado');	
	var CEP = g_form.getValue('cep');	
	
   if (newValue) {	 
	var EnderecoCompleto = Rua + ', Numero: ' + Numero + ' - Bairro: ' + Bairro + ' - ' + Cidade + '/' + Estado + ' - CEP:(' + CEP + ')';
	g_form.setValue('endereco_completo', EnderecoCompleto);
   }   
}]]></script>
